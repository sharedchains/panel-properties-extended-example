    "stringify": "^3.1.0",
    "watchify": "^3.11.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "dependencies": {
    "bpmn-js": "^2.4.0",
    "bpmn-js-properties-panel": "^0.25.2",
