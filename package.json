    "plugin",
    "moddle",
    "example"
  ],
  "author": "Luca Bonora <luca.bonora@sharedchains.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sharedchains/panel-properties-extended-example/issues"
  },
  "homepage": "https://github.com/sharedchains/panel-properties-extended-example#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.3",
    "camunda-modeler-plugin-helpers": "^3.1.0",
    "chai": "^4.2.0",
    "jsdom": "12.2.0",
    "jsdom-global": "3.0.2",
    "karma": "^6.4.1",
    "karma-browserify": "^5.3.0",
    "karma-mocha": "^1.3.0",
